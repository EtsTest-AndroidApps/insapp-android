<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.insapp.insapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <permission
        android:name="fr.insapp.insapp.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="fr.insapp.insapp.permission.C2D_MESSAGE" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/InsappMaterialTheme">

        <activity
            android:name="fr.insapp.insapp.IntroActivity">
        </activity>

        <activity
            android:name="fr.insapp.insapp.LegalConditionsActivity"
            android:label="@string/legal_conditions"
            android:parentActivityName=".IntroActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".IntroActivity" />
        </activity>

        <activity
            android:name="fr.insapp.insapp.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="fr.insapp.insapp.SigninActivity"
            android:label="@string/login"
            android:parentActivityName=".IntroActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".IntroActivity" />
        </activity>

        <activity
            android:name="fr.insapp.insapp.CreditsActivity"
            android:label="@string/credits"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name="fr.insapp.insapp.SettingsActivity"
            android:label="@string/settings"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name="fr.insapp.insapp.ProfileActivity"
            android:label="@string/profile"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name="fr.insapp.insapp.MainActivity"
            android:label="@string/app_name">
        </activity>

        <activity
            android:name="fr.insapp.insapp.SearchActivity"
            android:label="@string/search_results"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
        </activity>

        <activity
            android:name="fr.insapp.insapp.ClubActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name="fr.insapp.insapp.PostActivity"
            android:label="@string/post"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name="fr.insapp.insapp.EventActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name="fr.insapp.insapp.UsersActivity"
            android:label="@string/event_participants"
            android:parentActivityName=".EventActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".EventActivity" />
        </activity>
    </application>
</manifest>
